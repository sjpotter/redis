{
    "STATS": {
        "summary": "Return information about the function currently running (name, description, duration)",
        "complexity": "O(1)",
        "group": "scripting",
        "since": "7.0.0",
        "arity": 2,
        "container": "FUNCTION",
        "function": "functionStatsCommand",
        "command_flags": [
            "NOSCRIPT",
            "ALLOW_BUSY"
        ],
        "acl_categories": [
            "SCRIPTING"
        ],
        "command_tips": [
            "NONDETERMINISTIC_OUTPUT",
            "REQUEST_POLICY:ALL_SHARDS",
            "RESPONSE_POLICY:SPECIAL"
        ],
        "reply_schema": {
            "type": "object",
            "properties": {
                "running_script":{
                    "anyOf": [
                        {
                            "description": "script isn't running",
                            "type": "null"
                        },
                        {
                            "description": "running script info",
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "command": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "duration_ms": {
                                    "type": "number"
                                }
                            }
                        }
                    ]
                },
                "engines": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "object",
                        "properties": {
                            "libraries_count": {
                                "type": "integer"
                            },
                            "functions_count": {
                                "type": "integer"
                            }
                        }
                    }
                }
            }
        }
    }
}
